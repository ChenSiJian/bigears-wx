page {
  height: 100%;
}
.container {
  background: #d4e9c8;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.box {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border-radius: 50px;
    background-color: #f5f5f5;
    height: 68rpx;
    width: 700rpx;
    color: #999999;
}

.header {
    position: fixed;
    background-color: #d4e9c8;
    height: 100rpx;
    width: 100%;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    box-shadow: 0 0 3px 0 #e3e3e3;
    z-index: 99;
}

.head-img {
    width: 106rpx;
    height: 34rpx;
    margin-top: 40rpx;
}

.box image {
    margin-right: 10px;
    width: 14px;
    height: 14px;
    margin-bottom: -2px;
}
.cate-nav {
  position: fixed;
  left: 0;
  margin-top: 100rpx;
  z-index: 1000;
}

.cate-nav-body {
  height: 84rpx;
  white-space: nowrap;
  background: #d4e9c8;
  border-top: 1px solid rgba(0, 0, 0, 0.15);
  overflow: hidden;
}

.cate-nav .item {
  display: inline-block;
  height: 84rpx;
  min-width: 130rpx;
  padding: 0 15rpx;
}

.cate-nav .item .name {
  display: block;
  height: 84rpx;
  padding: 0 20rpx;
  line-height: 84rpx;
  color: #333;
  font-size: 30rpx;
  width: auto;
}

.cate-nav .item.active .name {
  color: #ab956d;
  border-bottom: 2px solid #ab956d;
}
.cate-item {
    margin-top: 100rpx;
    height: auto;
    overflow: hidden;
}
.like-container {
    /* margin-top:30rpx; */
    margin-top: 60rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #d4e9c8;
}

.headline {
    margin-top: 30rpx;
    width: 97rpx;
    height: 42rpx;
}
.preview-container {
    margin-top: 45rpx;
    width: 690rpx;
    display: flex;
    flex-direction: row;
    padding:0 30rpx;
    flex-wrap:wrap;
    justify-content: space-between;
}

.preview {
    margin-bottom: 30rpx;
}